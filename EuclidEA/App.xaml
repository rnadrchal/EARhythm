<prism:PrismApplication x:Class="EuclidEA.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EuclidEA"
             xmlns:prism="http://prismlibrary.com/" >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles.xaml"/>
                <ResourceDictionary Source="Resources/RhythmTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Rhythm/EuclidGeneratorTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Rhythm/BernoulliGeneratorTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Rhythm/PoissonGeneratorTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Rhythm/CellularAutomatonTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Rhythm/LSystemGeneratorTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Rhythm/PolyrhythmTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Rhythm/TrackChunkTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Pitch/ConstantPitchTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Pitch/RandomPitchTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Pitch/NormalDistributionPitchTemplate.xaml" />
                <ResourceDictionary Source="/Resources/Pitch/RecordPitchViewModel.xaml" />
                <ResourceDictionary Source="Resources/EvolutionOptionsTemplate.xaml" />
                <ResourceDictionary Source="Resources/FitnessServiceOptionsTemplate.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>

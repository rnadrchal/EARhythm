<prism:PrismApplication x:Class="EuclidEA.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EuclidEA"
             xmlns:prism="http://prismlibrary.com/" >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles.xaml"/>
                <ResourceDictionary Source="Resources/RhythmTemplate.xaml" />
                <ResourceDictionary Source="Resources/EuclidGeneratorTemplate.xaml" />
                <ResourceDictionary Source="Resources/BernoulliGeneratorTemplate.xaml" />
                <ResourceDictionary Source="Resources/PoissonGeneratorTemplate.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
